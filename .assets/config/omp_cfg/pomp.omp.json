{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "template": "({{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }}{{ end }}{{ .Full }}{{ end }}) ",
          "foreground": "#23D18B",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{.Icon}} ",
          "foreground": "#26C6DA",
          "type": "os",
          "style": "plain",
          "foreground_templates": [
            "{{ if .WSL }}#F5BDE6{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "# ",
          "foreground": "#FF6666",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "threshold": 1
          },
          "template": "<#CCCCCC>[</>{{ .FormattedMs }}<#CCCCCC>]</> ",
          "foreground": "#F5F543",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .SSHSession }}{{ .UserName }}<#4CD7FF>@</>{{ .HostName }} {{ end }}",
          "foreground": "#2CC7EE",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": "/",
            "style": "agnoster_short"
          },
          "leading_diamond": "\ue0b3\ue0b2",
          "trailing_diamond": "\ue0b0\ue0b1 ",
          "template": "<b>{{ .Path }}</b>",
          "background": "#16caa3",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "branch_max_length": 25,
            "cache_duration": "none",
            "commit_icon": "",
            "fetch_status": true,
            "tag_icon": ""
          },
          "template": "{{ .HEAD }} ",
          "foreground": "#62cf47",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FF4500{{ end }}",
            "{{ if gt .Ahead 0 }}#F5F543{{ end }}",
            "{{ if gt .Behind 0 }}#F5F543{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "bash": "$",
              "pwsh": "‚ù≠",
              "zsh": "%"
            }
          },
          "template": "{{ .Name }} ",
          "foreground": "#c0d123",
          "type": "shell",
          "style": "plain"
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "parse_kubeconfig": true
          },
          "template": "\u2388 {{ .Context }}{{ if and (.Namespace) (ne .Namespace \"default\") }}::<i>{{ .Namespace }}</i>{{ end }}",
          "foreground": "#2E6CE6",
          "type": "kubectl",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3
}
